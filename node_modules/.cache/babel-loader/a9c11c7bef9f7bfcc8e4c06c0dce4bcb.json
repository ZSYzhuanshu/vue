/div>\n        {{formatName(name)}}\n        <br>\n        <el-button type=\"primary\" size=\"mini\" @click=\"handleClick\">按 钮</el-button>{{count}}\n        <div>\n            <div class=\"book\"></div>\n            <div class=\"div1\"></div>\n            <div class=\"div2\"></div>\n            <div class=\"div3\"></div>\n        </div>\n    </div>\n</template>\n\n<script>\nexport default {\n    name: 'test',\n    component: {},\n    data() {\n        return {\n            arr: [\n                { name: 'asd', age: 18 },\n                { name: 'zxc', age: 43 },\n                { name: 'qwe', age: 25 },\n            ],\n            name: ['阿斯顿','按顺序','德芙'],\n            count: 0,\n        }\n    },\n    methods: {\n        handleClick() {\n            this.count+=1\n        },\n    },\n    computed: {\n        formatName() {\n            return function(val) {\n                return val.join('、')\n            }\n        }\n    },\n    watch: {\n        count: function(val) {\n            this.$message({\n                type: 'success',\n                message: `count改变了，变为${val}`\n            })\n        }\n    },\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.box{\n    .book{\n        width: 100px;\n        height: 100px;\n        background-color: rgb(20, 5, 5);\n        position: fixed;\n        right: 8px;\n        top: 25%;\n    }\n    .div1{\n        height: 300px;\n        background: #fff;\n        // background-color: rgb(211, 136, 136);\n    }\n    .div2{\n        position: relative;\n        z-index: 6;\n        box-shadow: 0 0 20px 20px #fff;\n        background: #fff;\n        height: 300px;\n        // background-color: rgb(92, 31, 31);\n    }\n    .div3{\n        height: 900px;\n        background: #fff;\n        // background-color: rgb(129, 68, 68);\n    }\n}\n</style>"],"sourceRoot":"src/views"}]}